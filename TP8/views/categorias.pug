extends layout

block content
  .w3-card-4
    header.w3-container.w3-light-blue
        h3 Categorias
  .w3-container.w3-white
    table.w3-table.w3-striped
        tr
            th.w3-text-teal Categoria
        each categoria in categorias
            tr
                td.w3-text-indigo
                    a(href='/premios?categoria=' + categoria._id)= categoria._id
