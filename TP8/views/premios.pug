extends layout

block content
  .w3-card-4
    header.w3-container.w3-light-blue
        h3 Premios
  .w3-container.w3-white
    table.w3-table.w3-striped
        tr
            th.w3-text-teal Categoria
            th.w3-text-teal Ano
        each premio in premios
            tr
                td.w3-text-indigo
                    a(href='/premios?categoria=' + premio.category + '&data=' + premio.year)= premio.category
                td.w3-text-indigo= premio.year
