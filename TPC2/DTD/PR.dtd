<?xml version="1.0" encoding="UTF-8"?>
<!ELEMENT project-record (key-name, title, begin-date, end-date, supervisor, workteam, abstract, deliverables)>
<!ELEMENT key-name (#PCDATA)>
<!ELEMENT title (#PCDATA)>
<!ATTLIST title 
            subtitle CDATA #IMPLIED>

<!ELEMENT begin-date (#PCDATA)>
<!ELEMENT end-date (#PCDATA)>
<!ELEMENT supervisor (#PCDATA) >

<!ELEMENT workteam  (collaborator+)>
<!ELEMENT collaborator (name, number, email, github)>

<!ELEMENT abstract (#PCDATA|link)* >

<!ELEMENT deliverables (deliverable)* >
<!ELEMENT deliverable (link) >

<!ELEMENT name (#PCDATA) >
<!ELEMENT number (#PCDATA) >
<!ELEMENT email (#PCDATA) >
<!ELEMENT github (link) >
<!ELEMENT link ANY>
<!ATTLIST link
            ref CDATA #REQUIRED>
